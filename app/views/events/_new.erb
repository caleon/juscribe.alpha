<% form_for :event, @event, :url => user_events_path(@user), :html => { :class => "eventForm", :method => :post, :multipart => true } do |f| %>
	<fieldset>
		<legend>New Event</legend>
		<%= render :partial => 'form', :locals => { :f => f } %>
		<%= submit_tag "Create" %>
	</fieldset>
<% end %>
<%= cancel_button %>