<h3>Article Pictures</h3>
<ul id="article-pictures" class="media_listing">
<% thumbs.each do |thumb| %>
	<li class="articlePicture pic">
		<%= link_to image_tag(thumb.public_filename, :class => dom_class(thumb), :alt => thumb.caption), picture_path_for(thumb.parent), :title => thumb.caption %>
	</li>
<% end %>
</ul>
