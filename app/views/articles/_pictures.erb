<h3>Article Pictures</h3>
<ul id="article-pictures" class="media_listing">
<% pictures.select{|pic| !pic.thumb.nil? }.each do |pic| %>
	<li class="articlePicture pic">
		<%= link_to image_tag(pic.thumb.public_filename, :class => dom_class(pic), :alt => pic.caption), picture_path_for(pic), :title => pic.caption %>
	</li>
<% end %>
</ul>
