<% feed = parseFeed(url) -%>
<div class="feed" style="display: none;" id="feed">
	<h4 class="descriptor"><span class="feedBlog">RSS: </span>From <%= link_to feed.feed_title, feed.feed_link %> by <%= feed.item_author %></h4>
	<h3><%= link_to feed.item_title, feed.item_link %></h3>
		<%# TODO: make this use the same formatter as feeds %>
		<%= format_feed(feed) %>
	<%= byline_for feed %>
</div>