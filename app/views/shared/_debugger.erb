<div id="debugger">
	<%= rounded_shim(:top) %>
	<p id="debugger-inner">
		Class of main object: <%= main_object.class.class_name %>: <%= main_object.display_name %>
		<br />
		Layoutable object: <%= @layoutable.class.class_name %>: <%= @layoutable.display_name %>
		<%= yield :debugger %>
	</p>
	<%= rounded_shim(:bottom) %>
</div>